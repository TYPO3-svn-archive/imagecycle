<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<general>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.sheet_general</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<mode>
						<TCEforms>
							<onChange>reload</onChange>
							<displayCond>EXT:dam:LOADED:true</displayCond>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.mode</label>
							<config>
								<type>select</type>
								<size>1</size>
								<maxitems>1</maxitems>
								<itemsProcFunc>tx_imagecycle_itemsProcFunc->getModes</itemsProcFunc>
							</config>
						</TCEforms>
					</mode>
					<images>
						<TCEforms>
							<displayCond>FIELD:mode:!IN:dam,dam_catedit</displayCond>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.images</label>
							<config>
								<type>group</type>
								<internal_type>file</internal_type>
								<allowed>files</allowed>
								<size>12</size>
								<maxitems>1000</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
								<allowed>gif,jpg,jpeg,png</allowed>
								<max_size>10240</max_size>
								<uploadfolder>uploads/tx_imagecycle</uploadfolder>
							</config>
						</TCEforms>
					</images>
					<hrefs>
						<TCEforms>
							<displayCond>FIELD:mode:!IN:dam,dam_catedit</displayCond>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.href</label>
							<config>
								<type>text</type>
								<wrap>OFF</wrap>
								<cols>48</cols>
								<rows>6</rows>
							</config>
						</TCEforms>
					</hrefs>
					<captions>
						<TCEforms>
							<displayCond>FIELD:mode:!IN:dam,dam_catedit</displayCond>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.caption</label>
							<config>
								<type>text</type>
								<wrap>OFF</wrap>
								<cols>48</cols>
								<rows>6</rows>
							</config>
						</TCEforms>
					</captions>
					<damimages>
						<TCEforms>
							<displayCond>FIELD:mode:=:dam</displayCond>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.damimages</label>
							<config>
								<type>group</type>
								<form_type>user</form_type>
								<userFunc><![CDATA[EXT:dam/lib/class.tx_dam_tcefunc.php:&tx_dam_tceFunc->getSingleField_typeMedia]]></userFunc>
								<userProcessClass><![CDATA[EXT:mmforeign/class.tx_mmforeign_tce.php:tx_mmforeign_tce]]></userProcessClass>
								<internal_type>db</internal_type>
								<allowed>tx_dam</allowed>
								<allowed_types>gif,jpg,jpeg,png</allowed_types>
								<prepend_tname>1</prepend_tname>
								<MM>tx_dam_mm_ref</MM>
								<MM_foreign_select>1</MM_foreign_select>
								<MM_opposite_field>1</MM_opposite_field>
								<MM_match_fields>
									<ident>imagecycle</ident>
								</MM_match_fields>
								<show_thumbs>1</show_thumbs>
								<size>10</size>
								<maxitems>1000</maxitems>
								<minitems>0</minitems>
								<autoSizeMax>30</autoSizeMax>
							</config>
						</TCEforms>
					</damimages>
					<damcategories>
						<TCEforms>
							<displayCond>FIELD:mode:=:dam_catedit</displayCond>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.damcategories</label>
							<config>
								<type>select</type>
								<form_type>user</form_type>
								<userFunc>EXT:dam/lib/class.tx_dam_tcefunc.php:tx_dam_tceFunc->getSingleField_selectTree</userFunc>
								<treeViewClass>EXT:dam/components/class.tx_dam_selectionCategory.php:tx_dam_selectionCategory</treeViewClass>
								<foreign_table>tx_dam_cat</foreign_table>
								<size>5</size>
								<autoSizeMax>25</autoSizeMax>
								<minitems>0</minitems>
								<maxitems>99</maxitems>
								<default></default>
							</config>
						</TCEforms>
					</damcategories>
				</el>
			</ROOT>
		</general>
		<settings>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.sheet_settings</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<imagewidth>
						<TCEforms>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.imagewidth</label>
							<config>
								<type>input</type>
								<size>6</size>
								<max>6</max>
								<eval>string</eval>
							</config>
						</TCEforms>
					</imagewidth>
					<imageheight>
						<TCEforms>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.imageheight</label>
							<config>
								<type>input</type>
								<size>6</size>
								<max>6</max>
								<eval>string</eval>
							</config>
						</TCEforms>
					</imageheight>
					<showcaption>
						<TCEforms>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.showcaption</label>
							<config>
								<type>check</type>
								<default>1</default>
							</config>
						</TCEforms>
					</showcaption>
					<random>
						<TCEforms>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.random</label>
							<config>
								<type>check</type>
								<default>1</default>
							</config>
						</TCEforms>
					</random>
					<stoponmousover>
						<TCEforms>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.stoponmousover</label>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</stoponmousover>
				</el>
			</ROOT>
		</settings>
		<movement>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.sheet_movement</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<type>
						<TCEforms>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.type</label>
							<config>
								<type>select</type>
								<size>1</size>
								<maxitems>1</maxitems>
								<itemsProcFunc>tx_imagecycle_itemsProcFunc->getEffects</itemsProcFunc>
							</config>
						</TCEforms>
					</type>
					<transition>
						<TCEforms>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.transition</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0"></numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">Quad</numIndex>
										<numIndex index="1">Quad</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">Cubic</numIndex>
										<numIndex index="1">Cubic</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">Quart</numIndex>
										<numIndex index="1">Quart</numIndex>
									</numIndex>
									<numIndex index="4" type="array">
										<numIndex index="0">Quint</numIndex>
										<numIndex index="1">Quint</numIndex>
									</numIndex>
									<numIndex index="5" type="array">
										<numIndex index="0">Expo</numIndex>
										<numIndex index="1">Expo</numIndex>
									</numIndex>
									<numIndex index="6" type="array">
										<numIndex index="0">Circ</numIndex>
										<numIndex index="1">Circ</numIndex>
									</numIndex>
									<numIndex index="7" type="array">
										<numIndex index="0">Sine</numIndex>
										<numIndex index="1">Sine</numIndex>
									</numIndex>
									<numIndex index="8" type="array">
										<numIndex index="0">Back</numIndex>
										<numIndex index="1">Back</numIndex>
									</numIndex>
									<numIndex index="9" type="array">
										<numIndex index="0">Bounce</numIndex>
										<numIndex index="1">Bounce</numIndex>
									</numIndex>
									<numIndex index="10" type="array">
										<numIndex index="0">Elastic</numIndex>
										<numIndex index="1">Elastic</numIndex>
									</numIndex>
								</items>
								<size>1</size>
								<maxitems>1</maxitems>
							</config>
						</TCEforms>
					</transition>
					<transitiondir>
						<TCEforms>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.transitiondir</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0"></numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">In</numIndex>
										<numIndex index="1">In</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">Out</numIndex>
										<numIndex index="1">Out</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">InOut</numIndex>
										<numIndex index="1">InOut</numIndex>
									</numIndex>
								</items>
								<size>1</size>
								<maxitems>1</maxitems>
							</config>
						</TCEforms>
					</transitiondir>
					<transitionduration>
						<TCEforms>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.transitionduration</label>
							<config>
								<type>input</type>
								<size>5</size>
								<max>5</max>
								<checkbox>0</checkbox>
								<eval>int</eval>
							</config>
						</TCEforms>
					</transitionduration>
					<displayduration>
						<TCEforms>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.displayduration</label>
							<config>
								<type>input</type>
								<size>5</size>
								<max>5</max>
								<checkbox>0</checkbox>
								<eval>int</eval>
							</config>
						</TCEforms>
					</displayduration>
					<delayduration>
						<TCEforms>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.delayduration</label>
							<config>
								<type>input</type>
								<size>5</size>
								<max>5</max>
								<checkbox>0</checkbox>
								<eval>int</eval>
							</config>
						</TCEforms>
					</delayduration>
					<sync>
						<TCEforms>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.sync</label>
							<config>
								<type>check</type>
								<default>1</default>
							</config>
						</TCEforms>
					</sync>
				</el>
			</ROOT>
		</movement>
		<special>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.sheet_special</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<options>
						<TCEforms>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.options</label>
							<config>
								<type>text</type>
								<wrap>OFF</wrap>
								<cols>48</cols>
								<rows>10</rows>
							</config>
						</TCEforms>
					</options>
				</el>
			</ROOT>
		</special>
	</sheets>
</T3DataStructure>
